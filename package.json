{
    "name": "tree-language-service",
    "displayName": "Tree_Language_Service",
    "description": "mol .tree language service",
    "publisher": "valikov",
    "version": "1.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/gituser3000/tree-language-service"
      },
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tree",
                "aliases": [
                    "mol view tree",
                    "tree"
                ],
                "extensions": [
                    ".tree"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tree",
                "scopeName": "source.tree",
                "path": "./syntaxes/tree.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "source-map": "^0.7.3",
        "typescript-language-server": "^0.4.0"
    },
    "devDependencies": {
        "@types/node": "^10.12.18",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1",
        "@types/vscode": "^1.32.0"
    }
}
